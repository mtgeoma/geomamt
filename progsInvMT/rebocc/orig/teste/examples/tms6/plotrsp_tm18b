rebocc_rsp2gmt -S23 -Dphi -I$FILETMdat -M$FILETMdix | psxy -JX -R -S$SYMBOLTM -G$COLORTM -K -O >> $OUTFILE
rebocc_rsp2gmt -S23 -Dphi -I$FILETMrsp | psxy -JX -R -W$PENTM -K -O >> $OUTFILE

## estação 24
#  basemap para rho
