EOF
rebocc_rsp2gmt -S23 -Drho -I$FILETMdat -M$FILETMdix | psxy -JX -R -S$SYMBOLTM -G$COLORTM -K -O >> $OUTFILE
rebocc_rsp2gmt -S23 -Drho -I$FILETMrsp | psxy -JX -R -W$PENTM -K -O >> $OUTFILE

